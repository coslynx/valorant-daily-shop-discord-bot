{
  "name": "valorant-daily-shop-discord-bot",
  "version": "1.0.0",
  "description": "A Discord bot that fetches and displays the Valorant daily shop for users.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "tsc && tsc-watch --onSuccess \"npm run build:prod\"",
    "build:prod": "tsc --module commonjs --outDir dist && tsc-watch --onSuccess \"npm run build:prod\"",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write \"src//.{ts,js,json}\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coslynx/valorant-daily-shop-discord-bot.git"
  },
  "author": "Coslynx",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.14",
    "discord.js": "^14.10.0",
    "dotenv": "^16.1.1",
    "jsonwebtoken": "^9.0.0",
    "puppeteer": "^20.0.0",
    "rate-limiter-flexible": "^2.1.0",
    "sqlite3": "^5.1.0"
  },
  "devDependencies": {
    "@types/cheerio": "^1.0.1",
    "@types/discord.js": "^14.10.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^20.5.4",
    "@types/puppeteer": "^10.5.0",
    "@types/sqlite3": "^5.0.3",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}